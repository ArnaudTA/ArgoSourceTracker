{
    "name": "argocd-source-tracker",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "workspaces": [
      "packages/*"
    ],
    "scripts": {
      "dev:frontend": "npm run dev -w frontend",
      "build:frontend": "npm run build -w frontend",
      "preview:frontend": "npm run preview -w frontend",
      "swag": "sh -c 'cd packages/backend/; swag init && cd ../frontend; npm run swag-external'"
    }
  }